{
  "name": "uncmt",
  "version": "0.0.0",
  "description": "Universal comment component for vue.",
  "type": "module",
  "homepage": "https://github.com/so1ve/uncmt#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/so1ve/uncmt.git"
  },
  "bugs": {
    "url": "https://github.com/so1ve/uncmt/issues"
  },
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs"
    }
  },
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "prepublishOnly": "nr build",
    "preview": "vite preview",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "release": "bumpp --commit --push --tag && pnpm publish",
    "typecheck": "tsc --noEmit"
  },
  "peerDependencies": {
    "vue": "^3.0.0"
  },
  "dependencies": {
    "@waline/client": "^2.14.2"
  },
  "devDependencies": {
    "@antfu/ni": "^0.20.0",
    "@so1ve/eslint-config": "^0.32.3",
    "@vitejs/plugin-vue": "^4.0.0",
    "bumpp": "^8.2.1",
    "eslint": "^8.29.0",
    "typescript": "^4.9.3",
    "unplugin-auto-import": "^0.12.1",
    "unplugin-vue-components": "^0.22.12",
    "vite": "^4.0.0",
    "vite-plugin-dts": "^1.7.1",
    "vue": "^3.2.45"
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "@babel/core"
      ]
    }
  }
}
